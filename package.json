{
  "name": "benchmarks",
  "description": "the npm cli benchmarking suite",
  "private": true,
  "dependencies": {
    "@octokit/rest": "^19.0.3",
    "npm-package-arg": "^9.0.2",
    "rimraf": "^3.0.2",
    "semver": "^7.3.7",
    "yargs": "^17.4.1"
  },
  "templateOSS": {
    "//@npmcli/template-oss": "This file is partially managed by @npmcli/template-oss. Edits may be overwritten.",
    "version": "3.5.0",
    "lockfile": true,
    "windowsCI": false,
    "ciVersions": [
      "16.x"
    ]
  },
  "scripts": {
    "lint": "eslint \"**/*.js\"",
    "postlint": "template-oss-check",
    "template-oss-apply": "template-oss-apply --force",
    "lintfix": "npm run lint -- --fix",
    "preversion": "npm test",
    "postversion": "git push origin --follow-tags",
    "snap": "tap",
    "test": "tap",
    "posttest": "npm run lint"
  },
  "tap": {
    "no-coverage": true,
    "timeout": 300
  },
  "author": "GitHub Inc.",
  "files": [
    "bin/",
    "lib/"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/npm/benchmarks.git"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "devDependencies": {
    "@npmcli/eslint-config": "^3.0.1",
    "@npmcli/template-oss": "3.6.0",
    "tap": "^16.0.1",
    "which": "^2.0.2"
  }
}
